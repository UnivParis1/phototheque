[class^="gallery-icon-"], [class*=" gallery-icon-"] {
  font-family: 'gallery-icon';
  font-style: normal;
  font-weight: normal;

  /* fix buttons height */
  line-height: 1em;

  /* you can be more comfortable with increased icons size */
  /* font-size: 120%; */
}

