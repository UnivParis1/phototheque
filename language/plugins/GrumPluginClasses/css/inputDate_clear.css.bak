/* clear css */
.ui-inputDate {
  background:#DDDDDD;
  border:1px solid #999999;
  color:#666666;
}
.ui-error, input[type='text'].ui-error, textarea.ui-error {
  background:#ff8080;
  border-color:#800000;
  color:#800000;
}
.ui-inputDate input[type="text"].ui-inputDate-time {
  border-left: 1px dotted #999999;
}
.ui-error input[type="text"].ui-inputDate-time {
  border-left: 1px dotted #800000;
}
