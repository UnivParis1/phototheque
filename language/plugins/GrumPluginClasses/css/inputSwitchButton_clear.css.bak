
/* clear css */
.ui-inputSwitchButton-unchecked {
  background:#005e89;
  border: 0px;
  -moz-border-radius:2px;
  -webkit-border-radius:2px;
  -khtml-border-radius:2px;
  border-radius:2px;
  color: #FFFFFF;
}

.ui-inputSwitchButton-checked {
  background:#d54e21;
  border: 0px;
  -moz-border-radius:2px;
  -webkit-border-radius:2px;
  -khtml-border-radius:2px;
  border-radius:2px;
  color: #FFFFFF;
}

