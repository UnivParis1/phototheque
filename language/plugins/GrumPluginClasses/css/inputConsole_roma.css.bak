/* roma css */
.ui-inputConsole {
  background:#444444;
  border:1px solid #666666;
  color:#999999;
}
.ui-inputConsole-history {
  border-color:#666666;
  color:#dddddd;
}

.ui-error, input[type='text'].ui-error {
  background:#ff8080;
  border-color:#800000;
  color:#800000;
}
.ui-inputConsole-historyBg {
  background:#444444;
}
.ui-inputConsole-history ul li:hover {
  background:#444444;
  cursor:pointer;
  color:#cccccc;
}
